<body>

  <!-- ========== END HEADER ========== -->

  <!-- ========== MAIN CONTENT ========== -->
  <main id='content' role='main'>
    <!-- Breadcrumb -->
    <div class='border-top border-bottom'>
      <div class='container py-3'>
        <!-- Breadcrumb -->
        <nav aria-label='breadcrumb'>
          <ol class='breadcrumb mb-0'>
            <li class='breadcrumb-item'>
              <a href='/v1'>Productos</a>
            </li>
            <li class='breadcrumb-item active' aria-current='page'>Resultados de
              la BÃºsqueda</li>
          </ol>
        </nav>
        <!-- End Breadcrumb -->
      </div>
    </div>
    <!-- End Breadcrumb -->

    <!-- Card Grid -->
    <div class='container content-space-2'>
      <div class='row col-lg-divider'>
        <div class='col-lg-3'>
          <!-- Navbar -->
            {{>_add_btn}}
          <!-- End Navbar -->
        </div>
        <!-- End Col -->

        <div class='col-lg-9'>
          <div class='ps-lg-2'>
            <!-- Search -->
            <div class='mb-4'>
              <div class='mb-3'>
                <h4>24 Resultados
                  <span class='text-body small'>para "Aquatop"</span></h4>
              </div>

              <form method="GET" action="/v1/products/search">
                <!-- Input Card -->
                <div class='input-card border'>
                  <div class='input-card-form'>
                    <label
                      for='searchAppsForm'
                      class='form-label visually-hidden'
                    >Search for apps</label>
                    <div class='input-group input-group-merge'>
                      <span class='input-group-prepend input-group-text'>
                        <i class='bi-search'></i>
                      </span>
                      <input type='text' name="q" class='form-control' id='searchAppsForm' placeholder='Buscar' aria-label='Search for apps'/>

                    </div>
                  </div>
                  <button type='submit button' class='btn btn-primary'>
                    <i class='bi-arrow-right'></i>
                  </button>
                </div>
                <!-- End Input Card -->
              </form>
            </div>
            <!-- End Search -->
            <h1>Search Results for "{{query}}"</h1>

            {{#if products.length}}
              <ul>
                {{#each products}}
                  <li>{{this.name}} by {{this.brand}}</li>
                {{/each}}
              </ul>
            {{else}}
              <p>No results found.</p>
            {{/if}}
